{
  "env": {
    "esm": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "macros"
      ]
    },
    "cjs": {
      "presets": [
        ["@babel/preset-env", {
          "modules": "cjs"
        }],
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "macros"
      ]
    }
  }
}