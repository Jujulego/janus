{
  "env": {
    "esm": {
      "presets": [
        "@babel/preset-typescript"
      ],
      "plugins": [
        "macros"
      ]
    },
    "cjs": {
      "presets": [
        ["@babel/preset-env", {
          "modules": "cjs",
          "targets": {
            "node": "14"
          }
        }],
        "@babel/preset-typescript"
      ],
      "plugins": [
        "macros"
      ]
    }
  }
}