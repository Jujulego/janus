{
  "name": "express-api",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "nodemon --watch src/** --exec 'yarn ts-node -r tsconfig-paths/register ./src/main.ts'",
    "start:local1": "nodemon --watch src/** --exec 'yarn ts-node -r tsconfig-paths/register ./src/main.ts local1'",
    "start:local2": "nodemon --watch src/** --exec 'yarn ts-node -r tsconfig-paths/register ./src/main.ts local2'",
    "start:local3": "nodemon --watch src/** --exec 'yarn ts-node -r tsconfig-paths/register ./src/main.ts local3'"
  },
  "dependencies": {
    "@jujulego/janus-gate": "^1.0.0-alpha.12",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^12.20.14",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
