{
  "name": "@jujulego/janus-types",
  "version": "1.0.4",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Jujulego/janus",
    "directory": "packages/types"
  },
  "scripts": {
    "lint": "eslint {src,tests}/**/*.*",
    "build": "yarn build:esm && yarn build:cjs && yarn build:types && yarn build:assets",
    "build:esm": "BABEL_ENV=esm babel --extensions '.ts' src --out-dir dist/esm",
    "build:cjs": "BABEL_ENV=cjs babel --extensions '.ts' src --out-dir dist/cjs",
    "build:types": "tsc -p tsconfig.types.json",
    "build:assets": "copyfiles -f src/config.schema.json dist",
    "test": "jest --passWithNoTests"
  },
  "engines": {
    "node": "^14"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.5",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-typescript": "7.15.0",
    "@babel/runtime": "7.15.4",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "babel-plugin-macros": "3.1.0",
    "copyfiles": "2.4.1",
    "eslint": "7.32.0",
    "eslint-plugin-jest": "24.4.2",
    "graphql": "15.6.0",
    "graphql.macro": "1.4.2",
    "jest": "27.2.4",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3"
  },
  "peerDependencies": {
    "graphql": "^15.6.0"
  }
}
